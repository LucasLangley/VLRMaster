# Ambiente de execução
NODE_ENV=development

# APIs do Valorant/Riot
RIOT_API_KEY=your_riot_api_key_here
VALORANT_API_BASE_URL=https://valorant-api.com
VALORANT_API_KEY=your_valorant_api_key_here

# Configurações de rede
DEV_PORT=3000
ELECTRON_PORT=3001

# Configurações de segurança
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_SALT=your_encryption_salt_here

# Configurações de analytics
GOOGLE_ANALYTICS_ID=your_ga_id_here
SENTRY_DSN=your_sentry_dsn_here

# Configurações de deploy
PRODUCTION_URL=https://your-app.netlify.app
NETLIFY_SITE_ID=your_netlify_site_id_here
NETLIFY_ACCESS_TOKEN=your_netlify_token_here

# Configurações de cache
API_CACHE_TTL=300
GAME_DETECTION_INTERVAL=5000

# Configurações de debug
DEBUG_MODE=false
LOG_LEVEL=info 